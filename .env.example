# Path to env file; update if you store it elsewhere.
ENV_FILE=.env

# Application metadata
APP_NAME=Technical Assessment API
ENVIRONMENT=local

# MySQL RDS configuration
MYSQL_HOST=your-rds-endpoint.rds.amazonaws.com
MYSQL_PORT=3306
MYSQL_USER=app_user
MYSQL_PASSWORD=super-secret-password
MYSQL_DB=technical_assessment

# Enable verbose SQL logging only for troubleshooting
ECHO_SQL=false
